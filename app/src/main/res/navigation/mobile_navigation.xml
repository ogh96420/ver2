<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.ver1.ui.dashboard.EventFragment"
        android:label="@string/title_eventpop"
        tools:layout="@layout/fragment_eventpop" />

    <fragment
        android:id="@+id/navigation_area"
        android:name="com.example.ver1.ui.AreaFragment"
        android:label="@string/title_area"
        tools:layout="@layout/fragment_area" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ver1.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_info"
        android:name="com.example.ver1.ui.info.InfoFragment"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_info" />

    <fragment
        android:id="@+id/navigation_more"
        android:name="com.example.ver1.ui.more.TalkFragment"
        android:label="@string/title_talk"
        tools:layout="@layout/fragment_talk" />


</navigation>